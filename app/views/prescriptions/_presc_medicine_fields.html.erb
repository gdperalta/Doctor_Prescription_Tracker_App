<div class="nested-fields">
	<%= f.hidden_field :_destroy %>
	<div>
		<%= f.number_field :prescription_id, type: :hidden %>
	</div>
	<div>
		<%= f.select :medicine_id, grouped_medicine_options, {}, {class: 'input_meds'}%>
		<%= f.range_field :discount, in: 0..100, class: 'discount_fields' %>
		<output></output>
	</div>
	<div>
		<%= link_to "Remove", '#', class: "remove_fields" %>
	</div>
</div>
